{
  "name": "react-withcontainer",
  "version": "2.1.1",
  "description": "HOC that allows ioc for presenter (Uis) to wrap themselves in containers passing themselves to the container as props.container.",
  "main": "dist/lib/commonjs/index.js",
  "module": "dist/lib/esm/index.js",
  "types": "dist/lib/esm/index.d.ts",
  "license": "MIT",
  "author": {
    "name": "Scott Bamford",
    "email": "scott@ambidect.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scottbamford/react-withcontainer.git"
  },
  "keywords": [
    "typescript",
    "react",
    "container",
    "presenter",
    "container-presenter",
    "ui",
    "ioc",
    "inversion",
    "inversion-of-control",
    "hoc",
    "higher-order-component",
    "high-order-component"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/scottbamford/react-withcontainer/issues"
  },
