  },
  "dependencies": {
    "react": "^16.8.6",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/react": "^16.8.16",
    "typescript": "^3.5.2",
    "rimraf": "^2.6.3"
